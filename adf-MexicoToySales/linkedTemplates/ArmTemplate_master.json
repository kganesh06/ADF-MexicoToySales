{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-MexicoToySales"
		},
		"ds_ablob_rawdata_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ds_ablob_rawdata'"
		},
		"ls_ablob_dataset_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_dataset'"
		},
		"ls_ablob_rawdata_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_rawdata'"
		},
		"ls_adls_rawdata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_rawdata'"
		},
		"ls_asql_mexico_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_asql_mexico_db'"
		},
		"ls_adls_rawdata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfpipelineadls.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-MexicoToySales_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ds_ablob_rawdata_connectionString": {
						"value": "[parameters('ds_ablob_rawdata_connectionString')]"
					},
					"ls_ablob_dataset_connectionString": {
						"value": "[parameters('ls_ablob_dataset_connectionString')]"
					},
					"ls_ablob_rawdata_connectionString": {
						"value": "[parameters('ls_ablob_rawdata_connectionString')]"
					},
					"ls_adls_rawdata_accountKey": {
						"value": "[parameters('ls_adls_rawdata_accountKey')]"
					},
					"ls_asql_mexico_db_connectionString": {
						"value": "[parameters('ls_asql_mexico_db_connectionString')]"
					},
					"ls_adls_rawdata_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_rawdata_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-MexicoToySales_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-MexicoToySales_ArmTemplate_0')]"
			]
		}
	]
}